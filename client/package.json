{
  "name": "proxijobs",
  "version": "0.0.1",
  "description": "QuickStart package.json from the documentation, supplemented with testing support",
  "scripts": {
    "start": "tsc --outDir js && concurrently \"tsc -w --outDir js\" \"lite-server\" ",
    "docker-build": "docker build -t ng2-quickstart .",
    "docker": "npm run docker-build && docker run -it --rm -p 3000:3000 -p 3001:3001 ng2-quickstart",
    "e2e": "tsc && concurrently \"http-server\" \"protractor protractor.config.js\"",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server",
    "postinstall": "typings install",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "tsc": "tsc --outDir js",
    "tsc:w": "tsc -w --outDir js",
    "typings": "typings",
    "webdriver:update": "webdriver-manager update"
  },
  "keywords": [],
  "author": "Thomas Luce",
  "license": "Apache-2.0",
  "dependencies": {
    "@angular/common": "^2.1.0",
    "@angular/compiler": "^2.1.0",
    "@angular/core": "^2.1.0",
    "@angular/forms": "^2.1.0",
    "@angular/http": "^2.1.0",
    "@angular/platform-browser": "^2.1.0",
    "@angular/platform-browser-dynamic": "^2.1.0",
    "@angular/router": "^3.1.0",
    "@angular/upgrade": "^2.1.0",
    "@types/node": "^6.0.45",
    "angular2-toaster": "^1.0.2",
    "bootstrap": "*",
    "browser-sync": "^2.17.3",
    "concurrently": "^3.1.0",
    "core-js": "*",
    "jquery": "^2.2.4",
    "lodash": "^4.16.4",
    "moment": "^2.13.0",
    "reflect-metadata": "*",
    "rxjs": "*",
    "semantic-ui": "^2.1.8",
    "systemjs": "^0.19.39",
    "typescript": "*",
    "typescript-collections": "*",
    "zone.js": "*"
  },
  "devDependencies": {
    "browser-sync": "*",
    "canonical-path": "*",
    "concurrently": "*",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "*",
    "gulp-cached": "*",
    "gulp-jsdoc": "*",
    "gulp-sass": "*",
    "gulp-typedoc": "*",
    "gulp-typescript": "^3.0.2",
    "http-server": "*",
    "lite-server": "*",
    "lodash": "*",
    "phantomjs": "*",
    "protractor": "^4.0.9",
    "rimraf": "*",
    "tslint": "*",
    "typedoc": "^0.5.0",
    "typescript": "*",
    "typings": "*"
  },
  "repository": {},
  "main": "app/js/main.js"
}
